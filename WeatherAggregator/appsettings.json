{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Api": {
    "ApiProviderConfigs": {
      "Weatherapi": {
        "ApiKey": "key",
        "DailyForecastEndpoint": "http://api.weatherapi.com/v1/forecast.json"
      },
      "OWM": {
        "ApiKey": "key",
        "DailyForecastEndpoint": "https://api.openweathermap.org/data/2.5/forecast"
      }
    },
    "WeatherProvidersPriority": [ "Weatherapi", "OWM" ]
  }
}
